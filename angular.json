{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "considera-dev": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "skipTests": true,
          "standalone": false
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/considera-dev",
      "sourceRoot": "projects/considera-dev/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/considera-dev",
            "index": "projects/considera-dev/src/index.html",
            "main": "projects/considera-dev/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/considera-dev/tsconfig.app.json",
            "assets": [
              "projects/considera-dev/src/favicon.ico",
              "projects/considera-dev/src/assets"
            ],
            "styles": [
              "projects/musique-hub/src/musique-theme.scss",
              "projects/considera-dev/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "options": {
            "proxyConfig": "projects/considera-dev/proxy.conf.json"
          },
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "considera-dev:build:production"
            },
            "development": {
              "buildTarget": "considera-dev:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "considera-dev:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "projects/considera-dev/src/favicon.ico",
              "projects/considera-dev/src/assets",
              "projects/considera-dev/src/assets/images"
            ],
            "styles": [
              "projects/musique-hub/src/musique-theme.scss",
              "projects/considera-dev/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
    "musique-hub": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "skipTests": true,
          "standalone": false
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/musique-hub",
      "sourceRoot": "projects/musique-hub/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/musique-hub",
            "index": "projects/musique-hub/src/index.html",
            "main": "projects/musique-hub/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/musique-hub/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/musique-hub/src/favicon.ico",
              "projects/musique-hub/src/assets"
            ],
            "styles": [
              "projects/musique-hub/src/styles.scss",
              "projects/musique-hub/src/musique-theme.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "musique-hub:build:production"
            },
            "development": {
              "buildTarget": "musique-hub:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "musique-hub:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/musique-hub/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/musique-hub/src/favicon.ico",
              "projects/musique-hub/src/assets"
            ],
            "styles": [
              "projects/musique-hub/src/styles.scss",
              "projects/musique-hub/src/musique-theme.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
